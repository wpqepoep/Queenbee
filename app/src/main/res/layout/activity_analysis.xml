<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/analysis_home"
    tools:context=".Analysis">

    <LinearLayout
        android:id="@+id/L1"
        android:layout_marginTop="100dp"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/analysis_t"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="各項分析"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/analysis"
            android:text="Detail"
            android:textSize="20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <FrameLayout
        android:id="@+id/fl1"
        android:layout_below="@+id/L1"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/ibsugar"
            android:layout_marginVertical="5dp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/green_round"/>

        <Button
            android:id="@+id/btnsugar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="糖水"
            android:textColor="#8A000000"
            android:background="#00000000"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/iv1"
            android:layout_gravity="bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/line" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/fl2"
        android:layout_below="@+id/fl1"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_marginVertical="5dp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/brown_round"/>

        <Button
            android:id="@+id/btnbee"
            android:text="蜂量"
            android:textSize="20sp"
            android:textColor="#8A000000"
            android:background="#00000000"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <ImageView
            android:id="@+id/iv2"
            android:layout_gravity="bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/line" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/fl3"
        android:layout_below="@+id/fl2"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_marginVertical="5dp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/dbrown_round"/>

        <Button
            android:id="@+id/btnbeef"
            android:text="蜂糧"
            android:textSize="20sp"
            android:textColor="#8A000000"
            android:background="#00000000"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <ImageView
            android:id="@+id/iv3"
            android:layout_gravity="bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/line" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/fl4"
        android:layout_below="@+id/fl3"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_marginVertical="5dp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/lbrownd_round"/>

        <Button
            android:id="@+id/btnflower"
            android:text="花粉量"
            android:textSize="20sp"
            android:textColor="#8A000000"
            android:background="#00000000"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp" />

        <ImageView
            android:id="@+id/iv4"
            android:layout_gravity="bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/line" />

    </FrameLayout>



    <LinearLayout
        android:id="@+id/L2"
        android:layout_marginTop="55dp"
        android:layout_marginRight="-5dp"
        android:layout_toLeftOf="@+id/L7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/L1">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/green_6"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/L3"
        android:layout_marginTop="0dp"
        android:layout_toLeftOf="@+id/L7"
        android:layout_marginRight="-5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/L2">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/dbrown_6"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/L4"
        android:layout_marginTop="0dp"
        android:layout_toLeftOf="@+id/L7"
        android:layout_marginRight="-5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/L3">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/lbrownd_6"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/L7"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/L1">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/brown_6"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/L5"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/L7">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/skin_6"/>
    </LinearLayout>

    <LinearLayout
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/L5">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/orange_6"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/L8"
        android:layout_marginTop="-25dp"
        android:layout_marginLeft="-5dp"
        android:layout_toRightOf="@+id/L7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/L5">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/lskin_6"/>
    </LinearLayout>

    <FrameLayout
        android:id="@+id/fl5"
        android:layout_below="@+id/L1"
        android:layout_marginTop="0dp"
        android:layout_marginRight="10dp"
        android:layout_alignParentRight="true"
        android:layout_width="150dp"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/iv5"
            android:layout_gravity="bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/line"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp" />

        <Button
            android:id="@+id/btnqueenbee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="-15dp"
            android:layout_marginLeft="10dp"
            android:text="女王蜂週期"
            android:textSize="20sp"
            android:textColor="#8A000000"
            android:background="#00000000"/>


        <ImageView
            android:layout_marginVertical="4dp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="110dp"
            android:layout_marginLeft="90dp"
            android:background="@drawable/skin_round"
             />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/fl6"
        android:layout_below="@+id/fl5"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_alignParentRight="true"
        android:layout_width="150dp"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/iv6"
            android:layout_gravity="bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/line"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp" />

        <Button
            android:id="@+id/btnqueen"
            android:text="王台"
            android:textSize="20sp"
            android:textColor="#8A000000"
            android:background="#00000000"
            android:layout_gravity="center"
            android:layout_marginStart="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp" />


        <ImageView
            android:layout_marginVertical="4dp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="110dp"
            android:layout_marginLeft="90dp"
            android:background="@drawable/lskin_round"
            />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/fl7"
        android:layout_below="@+id/fl6"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_alignParentRight="true"
        android:layout_width="150dp"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/iv7"
            android:layout_gravity="bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/line"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp" />

        <Button
            android:id="@+id/btnhoney"
            android:text="蜜量"
            android:textSize="20sp"
            android:textColor="#8A000000"
            android:background="#00000000"
            android:layout_gravity="center"
            android:layout_marginStart="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp" />


        <ImageView
            android:layout_marginVertical="4dp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="110dp"
            android:layout_marginLeft="90dp"
            android:background="@drawable/orange_round"
            />

    </FrameLayout>

</RelativeLayout>